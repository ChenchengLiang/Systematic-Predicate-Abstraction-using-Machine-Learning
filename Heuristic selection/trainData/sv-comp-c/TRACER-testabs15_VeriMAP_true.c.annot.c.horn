inv_main9(n, arg0, arg1, arg2, TRACER_NONDET) :- n >= 0 & 0 = arg0 & 0 = arg1 & 0 = arg2.
inv_main9(n, arg3, arg4, arg5, TRACER_NONDET) :- inv_main9(n, i, a, b, TRACER_NONDET), TRACER_NONDET != 0 & n - i >= 1 & i + 1 = arg3 & a + 1 = arg4 & b + 2 = arg5.
inv_main9(n, arg6, arg7, arg8, TRACER_NONDET) :- inv_main9(n, i, a, b, 0), n - i >= 1 & TRACER_NONDET = 0 & i + 1 = arg6 & a + 2 = arg7 & b + 1 = arg8.
false :- inv_main9(n, i, a, b, TRACER_NONDET), a + b != 3*n & !(n - i >= 1).
