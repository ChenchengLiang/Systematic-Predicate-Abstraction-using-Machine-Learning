inv_main5(n, arg0, arg1) :- 1 = arg0 & 1 = arg1 & 0 = arg2.
inv_main8(n, k, i, arg3) :- inv_main5(n, k, i), n - i >= 1 & 0 = arg3.
inv_main5(n, k, arg4) :- inv_main8(n, k, i, j), !(i - j >= 1) & i + 1 = arg4.
inv_main8(n, arg5, i, arg6) :- inv_main8(n, k, i, j), i - j >= 1 & k + i - j = arg5 & j + 1 = arg6.
false :- inv_main5(n, k, i), !(k >= n | n - i >= 1).
