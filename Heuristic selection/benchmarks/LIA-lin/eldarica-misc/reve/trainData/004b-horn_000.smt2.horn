inv1(A,B,C,D,E,F,G,H) :- (I = (-1 + E)),(G = H),(D >= I),\+((A >= B)),((J + I) = F),inv1(A,B,C,D,I,J,G,H).
inv1(A,B,C,D,E,F,G,H) :- ((I + J) = C),(K = (-1 + E)),(J = (-1 + B)),(G = H),(A >= J),(D >= K),((L + K) = F),inv1(A,J,I,D,K,L,G,H).
inv1(A,B,C,D,E,F,G,H) :- (I = (-1 + B)),(G = H),\+((D >= E)),(A >= I),((J + I) = C),inv1(A,I,J,D,E,F,G,H).
inv1(A,B,C,D,E,F,G,H) :- (E = 1),(C = 0),(B = 0),(A = D),(F = 0),(G = A),(H = D).
false :- \+((A = B)),\+((C >= D)),\+((E >= F)),(G = H),inv1(E,F,A,C,D,B,G,H).